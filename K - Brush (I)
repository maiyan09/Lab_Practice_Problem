//K - Brush (I)
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int t;
    scanf("%d", &t);
    
    for (int j = 1; j <= t; j++)
    {  
        int n, sum = 0;
        getchar();
        scanf("%d", &n);
        int num[n];
        for (int i = 0; i < n; i++)
        {
            scanf("%d", &num[i]);
            if(num[i] >= 0)
            {
                sum += num[i];
            }
            
        }
        printf("Case %d: %d\n", j, sum);
    }
}
